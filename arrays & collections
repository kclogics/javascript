3)Array.find // Array.findIndex
let add1 = Array.of(2, 3, 100, 200);
let add2 = Array.of(200, 200, 300);

let addSum = Array.of(...add1, ...add2);
let addSumAll = addArray(...addSum);

function findOver() {
  let addSumOver = add1.find(over => over > 1Array);
  console.log("addSumOver", addSumOver);
}

4) Array.forEach

let initial = 100;

function addSumPlus(x) {
  initial = x + initial;
}

let dummy1 = Array.of(90, 8, 3);

dummy1.forEach(addSumPlus);
console.log("initial", initial);


